client
dev tun
proto udp
auth-user-pass
resolv-retry 30
remote-random
nobind
tun-mtu 1500
tun-mtu-extra 32
mssfix 1450
persist-key
persist-tun
ping 15
ping-restart 0
ping-timer-rem
reneg-sec 0
comp-lzo no

explicit-exit-notify 3

remote-cert-tls server

#mute 10000

verb 3
pull
fast-io
cipher AES-256-CBC
auth SHA512

